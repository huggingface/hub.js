{
	"$id": "/inference/schemas/chat-completion/stream_output.json",
	"$schema": "http://json-schema.org/draft-06/schema#",
	"description": "Chat Completion Stream Output",
	"title": "ChatCompletionStream_output",
	"type": "object",
	"required": ["id", "object", "created", "model", "system_fingerprint", "choices"],
	"properties": {
		"choices": {
			"type": "array",
			"items": {
				"$ref": "#/$defs/ChatCompletionStream_outputChatCompletionChoice"
			}
		},
		"created": {
			"type": "integer",
			"format": "int64",
			"example": "1706270978",
			"minimum": 0
		},
		"id": {
			"type": "string"
		},
		"model": {
			"type": "string",
			"example": "mistralai/Mistral-7B-Instruct-v0.2"
		},
		"object": {
			"type": "string"
		},
		"system_fingerprint": {
			"type": "string"
		}
	},
	"$defs": {
		"ChatCompletionStream_output": {
			"type": "object",
			"required": ["id", "object", "created", "model", "system_fingerprint", "choices"],
			"properties": {
				"choices": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/ChatCompletionStream_outputChatCompletionChoice"
					}
				},
				"created": {
					"type": "integer",
					"format": "int64",
					"example": "1706270978",
					"minimum": 0
				},
				"id": {
					"type": "string"
				},
				"model": {
					"type": "string",
					"example": "mistralai/Mistral-7B-Instruct-v0.2"
				},
				"object": {
					"type": "string"
				},
				"system_fingerprint": {
					"type": "string"
				}
			}
		},
		"ChatCompletionStream_outputChatCompletionChoice": {
			"type": "object",
			"required": ["index", "delta"],
			"properties": {
				"delta": {
					"$ref": "#/$defs/ChatCompletionStream_outputChatCompletionDelta"
				},
				"finish_reason": {
					"type": "string",
					"nullable": true
				},
				"index": {
					"type": "integer",
					"format": "int32",
					"minimum": 0
				},
				"logprobs": {
					"allOf": [
						{
							"$ref": "#/$defs/ChatCompletionStream_outputChatCompletionLogprobs"
						}
					],
					"nullable": true
				}
			}
		},
		"ChatCompletionStream_outputChatCompletionDelta": {
			"type": "object",
			"required": ["role"],
			"properties": {
				"content": {
					"type": "string",
					"example": "What is Deep Learning?",
					"nullable": true
				},
				"role": {
					"type": "string",
					"example": "user"
				},
				"tool_calls": {
					"allOf": [
						{
							"$ref": "#/$defs/ChatCompletionStream_outputDeltaToolCall"
						}
					],
					"nullable": true
				}
			}
		},
		"ChatCompletionStream_outputDeltaToolCall": {
			"type": "object",
			"required": ["index", "id", "type", "function"],
			"properties": {
				"function": {
					"$ref": "#/$defs/ChatCompletionStream_outputFunction"
				},
				"id": {
					"type": "string"
				},
				"index": {
					"type": "integer",
					"format": "int32",
					"minimum": 0
				},
				"type": {
					"type": "string"
				}
			}
		},
		"ChatCompletionStream_outputFunction": {
			"type": "object",
			"required": ["arguments"],
			"properties": {
				"arguments": {
					"type": "string"
				},
				"name": {
					"type": "string",
					"nullable": true
				}
			}
		},
		"ChatCompletionStream_outputChatCompletionLogprobs": {
			"type": "object",
			"required": ["content"],
			"properties": {
				"content": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/ChatCompletionStream_outputChatCompletionLogprob"
					}
				}
			}
		},
		"ChatCompletionStream_outputChatCompletionLogprob": {
			"type": "object",
			"required": ["token", "logprob", "top_logprobs"],
			"properties": {
				"logprob": {
					"type": "number",
					"format": "float"
				},
				"token": {
					"type": "string"
				},
				"top_logprobs": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/ChatCompletionStream_outputChatCompletionTopLogprob"
					}
				}
			}
		},
		"ChatCompletionStream_outputChatCompletionTopLogprob": {
			"type": "object",
			"required": ["token", "logprob"],
			"properties": {
				"logprob": {
					"type": "number",
					"format": "float"
				},
				"token": {
					"type": "string"
				}
			}
		}
	}
}
